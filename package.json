{
  "name": "@mojojs/core",
  "version": "0.4.5",
  "description": "Real-time web framework",
  "keywords": [
    "web",
    "framework"
  ],
  "author": "Sebastian Riedel <kraih@mojolicious.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojolicious/mojo.js.git"
  },
  "bugs": {
    "url": "https://github.com/mojolicious/mojo.js/issues"
  },
  "homepage": "https://mojojs.org",
  "scripts": {
    "bench": "npx concurrently -k -s first \"NODE_ENV=production node benchmarks/hello/hello-mojo.js server\" \"npx autocannon -c 100 -d 5 -p 10 localhost:3000\"",
    "build": "npx tsc --build ./ ./test/support/ts-app",
    "build:bench": "npm run build && npm run bench",
    "build:clean": "npm run clean && npm run build",
    "build:coverage": "npm run build && npm run coverage",
    "build:lint": "npm run build && npm run lint",
    "build:test": "npm run build && npm test",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -r tsconfig.tsbuildinfo test/support/ts-app/tsconfig.tsbuildinfo lib test/support/ts-app/lib",
    "coverage": "tap --node-arg='--no-warnings' --node-arg='--experimental-loader' --node-arg='@istanbuljs/esm-loader-hook' test/*.js",
    "coverage:ci": "npm run coverage -- --no-check-coverage && tap --no-check-coverage --coverage-report=text-lcov > .lcov.info",
    "lint": "eslint \"test/*.js\" \"src/**/*.ts\" \"src/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "publish:major": "npm version major && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "test": "tap --no-coverage test/*.js"
  },
  "exports": "./lib/core.js",
  "types": "./lib/core.d.ts",
  "type": "module",
  "files": [
    "bin/",
    "lib/",
    "vendor/"
  ],
  "bin": {
    "mojo": "./bin/mojo.js"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^=0.1.2",
    "@types/busboy": "^0.2.3",
    "@types/cheerio": "^0.22.29",
    "@types/cookie": "^0.4.0",
    "@types/ejs": "^3.0.6",
    "@types/lru-cache": "^5.1.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^16.0.0",
    "@types/nopt": "^3.0.29",
    "@types/stack-utils": "^2.0.0",
    "@types/tap": "^15.0.3",
    "@types/tough-cookie": "^4.0.0",
    "@types/ws": "^7.4.5",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "autocannon": "^7.3.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "prettier": "2.3.2",
    "typescript": "^=4.3.2"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@mojojs/path": "^1.1.2",
    "ajv": "^8.5.0",
    "busboy": "^0.3.1",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.10",
    "cookie": "^0.4.1",
    "ejs": "^3.1.6",
    "escape-string-regexp": "^5.0.0",
    "form-data": "^4.0.0",
    "lru-cache": "^6.0.0",
    "mime-types": "^2.1.30",
    "nopt": "^5.0.0",
    "stack-utils": "^2.0.3",
    "tap": "^15.0.6",
    "tough-cookie": "^4.0.0",
    "ws": "^8.1.0"
  }
}
